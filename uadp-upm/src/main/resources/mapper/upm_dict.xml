<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="upm.dict" >
  
  <!-- 查询数据字典内容 -->
  <select id="queryDictByKey" resultType="com.upbos.upm.entity.Dict">
  	select * from upm_dict where _key in (#{key})
  </select>
  
  <!-- 查询数据字典内容 -->
  <select id="queryDictByKeys" resultType="com.upbos.upm.entity.Dict">
  	select * from upm_dict where _key in (#{keys})
  </select>
</mapper>